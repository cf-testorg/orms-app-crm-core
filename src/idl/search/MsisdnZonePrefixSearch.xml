<MODEL>
    <IMPORT>com.redknee.framework.xhome.webcontrol.*</IMPORT>
    <IMPORT>com.redknee.framework.xhome.msp.*</IMPORT>
    <IMPORT>com.redknee.app.crm.web.control.*</IMPORT>
    
    <ENTITY generate-home="false" generate-table-webcontrol="false">
        <PACKAGE>com.redknee.app.crm.bean.search</PACKAGE>
        <NAME>MsisdnZonePrefixSearch</NAME>
        <LABEL>Destination Zone Search</LABEL>
        <HELP>A search bean for MsisdnZonePrefix classes.</HELP>

        <PROPERTY>
            <NAME>identifier</NAME>
            <LABEL>ID</LABEL>
            <WIDTH>10</WIDTH>
            <TYPE>long</TYPE>
            <DEFAULT>-1</DEFAULT>
            <web-control>new OptionalLongWebControl(-1)</web-control>
            <HELP>The unique identifier of this set of mobile number prefixes.</HELP>
        </PROPERTY>

        <PROPERTY>
            <NAME>description</NAME>
            <LABEL>Description</LABEL>
            <HELP>A description of the set of mobile number prefixes.</HELP>
        </PROPERTY>
    </ENTITY>
</MODEL>

